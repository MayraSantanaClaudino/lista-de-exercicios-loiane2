import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Digite um n√∫mero: ");
		double valor = scan.nextDouble();
		
		if (valor == (int)valor){
		    System.out.println("Inteiro");
		} else {
		    System.out.println("Decimal");
		}
	}
}
